// This juttle program contains common controls used by the other AWS
// juttle programs. It is not meant to be run by itself.

import "aws_control_product.juttle" as control_product;

export input aws_agg_in: dropdown
    -label 'Aggregate'
    -juttle "streams -space 'aws' -recent true -source_type 'metric' aws_product='${control_product.aws_product_in}' and metric_type='AWS Aggregate' | reduce by aggregate | sort 'aggregate'"
    -default '(null)';



